jQuery(document).ready(function(){
	 var lastToggle = '';
	 jQuery(".cursos-filter").live("click", function(){
		  if ( lastToggle.length && lastToggle.attr("rel") != jQuery(this).attr("rel") ){ 
		  	jQuery("#"+lastToggle.attr("rel")).slideToggle(300); lastToggle = jQuery(this);
		  } else if( lastToggle.length && lastToggle.attr("rel") == jQuery(this).attr("rel") ) {
			lastToggle = '';
		  } else {
			lastToggle = jQuery(this);
		  }
          jQuery("#"+jQuery(this).attr("rel")).slideToggle(300);
          return false;
     });
	 
	 function hide_all_filters(noHide){
		 if (noHide != jQuery("#edit-field-temas-tid-wrapper")) jQuery("#edit-field-temas-tid-wrapper").hide();
		 if (noHide != jQuery("#edit-field-habilidades-tid-wrapper")) jQuery("#edit-field-habilidades-tid-wrapper").hide();
		 if (noHide != jQuery("#edit-field-actividades-tid-wrapper")) jQuery("#edit-field-actividades-tid-wrapper").hide();
		 if (noHide != jQuery("#edit-field-tipo-curso-value-wrapper")) jQuery("#edit-field-tipo-curso-value-wrapper").hide();
		 if (noHide != jQuery("#edit-field-tipo-de-recurso-value-wrapper")) jQuery("#edit-field-tipo-de-recurso-value-wrapper").hide();
	 }
	 
	 jQuery("html").click(function(){
		hideBoxes();
	 });

	jQuery(document).ajaxComplete(function(event, XMLHttpRequest) {
	  if (XMLHttpRequest.responseText.substring(2,6) != 'pong'){ 
		  // Clear comment text field after save !
		  if (jQuery('#edit-drupal-comment')){
		  	jQuery('#edit-drupal-comment').val('');
		  }
		  jQuery('*[id^=edit-drupal-wall-comment]').val('');
	  }
	  initPackery();
	});
	
	checkFlags();
	
	jQuery(document).ajaxComplete(function(event, XMLHttpRequest) {
	  checkFlags();
	});
	
	jQuery(".flags a.flag-action").live("click",function(){
	  var count = jQuery(this).closest("div.flags").find("span.count").html();
	  jQuery(this).closest("div.flags").find("span.count").html(parseInt(count)+1);
	});
	
	jQuery(".flags a.unflag-action").live("click",function(){
		var count = jQuery(this).closest("div.flags").find("span.count").html();
		jQuery(this).closest("div.flags").find("span.count").html(parseInt(count)-1);
	});
	
	if (jQuery('#block-views-message-block-1 .view-content')){
		jQuery('#block-views-message-block-1 .view-content').niceScroll({cursorcolor:'#419cd1', autohidemode:false});
	}
	
	if (jQuery('#block-views-cursos-en-linea-block-1 .view-content')){
		jQuery('#block-views-cursos-en-linea-block-1 .view-content').niceScroll({cursorcolor:'#419cd1', autohidemode:false});
	}
	
	if (jQuery('#block-views-recursos-educativos-block-1 .view-content')){
		jQuery('#block-views-recursos-educativos-block-1 .view-content').niceScroll({cursorcolor:'#419cd1', autohidemode:false});
	}
	
	/* Packery init */
	initPackery(); 
	
	if (jQuery('#slideshow').length > 0){
	jQuery('#slideshow').cycle({ 
	    fx:      'scrollHorz', 
	    timeout:  0, 
	    speed:  300,
	    slideExpr: '.slide',
	    next:   '#next', 
    	prev:   '#prev',
    	pager:  '#nav',
	    pagerAnchorBuilder: function(idx, slide) { 
	    	var text=""; var css="";

	    		 if(idx==0){text="Capacítate y trabaja"; color="blue"; icon="fa-desktop";}
	    	else if(idx==1){text="Inicia y mejora tu negocio"; color="green"; icon="fa-globe";}
	    	else if(idx==2){text="Involúcrate, participa y crece"; color="orange"; icon="fa-comments";}

	        return '<li class="' + color + '"><a href="#" class="'+ icon +'">' + text + '</a></li>'; 
	    }
	});
	}
