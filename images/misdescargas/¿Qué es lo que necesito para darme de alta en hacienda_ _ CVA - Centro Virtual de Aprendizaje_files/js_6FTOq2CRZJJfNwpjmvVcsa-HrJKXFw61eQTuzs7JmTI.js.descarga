	var animating = false;
	jQuery("#slideshow .slide .areas").each(function(){
		jQuery(this).mouseenter(function(){
		    jQuery(this).find(".info").stop().animate({height:'100px', },100, function(e){
		    	jQuery(this).addClass("active");
		    });
		}).mouseleave(function(){
			jQuery(this).find(".info").stop().animate({height:'43px', },100, function(e){
				jQuery(this).removeClass("active");
			});
		});
	});
	
	jQuery(".tab").click(function(){
		jQuery(".tab-content").hide();
		jQuery(".tab").removeClass("selected");
		jQuery(this).addClass("selected");
		jQuery('.tabcontent-'+jQuery(this).attr("id")).show();
		return false;
	});
	
